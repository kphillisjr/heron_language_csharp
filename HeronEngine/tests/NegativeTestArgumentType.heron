module NegativeTestArgumentType
{
    imports
    {
        console = new Heron.Windows.Console();
    }
    methods
    {
        Fails(x : SomeObject)
        {
            WriteLine("Should never happen");
        }
        Main()
        {
            WriteLine("Expect exception: failed to cast from a OtherObject to a SomeObject");
            var x = new OtherObject();
            Fails(x);
        }
    }
}

class SomeObject
{
}

class OtherObject
{
}