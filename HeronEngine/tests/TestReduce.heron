module TestReduce
{
  inherits
  {
    Heron.Windows.Console;
  }
  methods
  {    
    Main()
    {
      var output;
      foreach (i in 1..10)
      {
        var input = 1..5000;
        output = reduce (a, b in input) a + b;
      }
      WriteLine(output[0].ToString());
    }
  }
}