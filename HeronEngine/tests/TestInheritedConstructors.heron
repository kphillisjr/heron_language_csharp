module TestInheritedConstructor
{
	methods
	{
		Main()
		{
			new SomeClass();
		}
	}
}

class OtherClass {	
	methods {
		Constructor() {
			Console.WriteLine("Hello World");
		}
	}
}

class SomeClass {
	inherits {
		OtherClass;
	}
	methods {
		Constructor() {
			base.Constructor();
		}
	}
}
}